plug_pg:mono_in
	a pg:MonoGroup, pg:InputGroup ;
	lv2:symbol "mono_in" ;
	rdfs:label "Mono Input" ;
	pg:element [
		lv2:index 0 ;
		lv2:designation pg:center
	]
	.

plug_pg:mono_out
	a pg:MonoGroup, pg:OutputGroup ;
	lv2:symbol "mono_out" ;
	rdfs:label "Mono Output" ;
	pg:element [
		lv2:index 0 ;
		lv2:designation pg:center
	]
	.

plug:comp_delay_mono
	a lv2:DelayPlugin, doap:Project ;
	doap:name "LSP Delay Compensator Mono" ;
	lv2:minorVersion 0 ;
	lv2:microVersion 27 ;
	doap:developer plug_dev:v_sadovnikov ;
	doap:maintainer plug_dev:lsp ;
	doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
	lv2:binary <lsp-plugins-lv2.so> ;
	ui:ui plug_ui:comp_delay_mono ;
	lv2:requiredFeature urid:map ;
	lv2:optionalFeature lv2:hardRTCapable, hcid:queue_draw, work:schedule, opts:options, state:mapPath ;
	lv2:extensionData state:interface, work:interface, hcid:interface ;
	opts:supportedOption ui:updateRate ;
	pg:mainInput plug_pg:mono_in ;
	pg:mainOutput plug_pg:mono_out ;
	dc:replaces <urn:ladspa:5002065> ;

	lv2:port [
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "Input" ;
		pg:group plug_pg:mono_in ;
		lv2:designation pg:center ;
	] , [
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Output" ;
		pg:group plug_pg:mono_out ;
		lv2:designation pg:center ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "enabled" ;
		lv2:name "Enabled" ;
		lv2:designation lv2:enabled ;
		lv2:portProperty lv2:toggled ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
		lv2:default 1 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "mode" ;
		lv2:name "Mode" ;
		lv2:portProperty lv2:integer, lv2:enumeration, pp:hasStrictBounds ;
		lv2:scalePoint
			[ rdfs:label "Samples"; rdf:value 0 ] ,
			[ rdfs:label "Distance"; rdf:value 1 ] ,
			[ rdfs:label "Time"; rdf:value 2 ] ;
		lv2:minimum 0 ;
		lv2:maximum 2 ;
		lv2:default 0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "ramp" ;
		lv2:name "Ramping" ;
		lv2:portProperty lv2:toggled ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
		lv2:default 0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "samp" ;
		lv2:name "Samples" ;
		units:unit [
			a units:Unit ;
			rdfs:label "samples" ;
			units:symbol "samp" ;
			units:render "%.0f samp" ;
		] ;
		lv2:portProperty lv2:integer, pp:hasStrictBounds ;
		lv2:minimum 0 ;
		lv2:maximum 10000 ;
		lv2:default 0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "m" ;
		lv2:name "Meters" ;
		units:unit units:m ;
		lv2:portProperty lv2:integer, pp:hasStrictBounds ;
		lv2:minimum 0 ;
		lv2:maximum 200 ;
		lv2:default 0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "cm" ;
		lv2:name "Centimeters" ;
		units:unit units:cm ;
		lv2:portProperty pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 100.000000 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "t" ;
		lv2:name "Temperature" ;
		units:unit [
			a units:Unit ;
			rdfs:label "degrees Celsium" ;
			units:symbol "°C" ;
			units:render "%.2f °C" ;
		] ;
		lv2:portProperty pp:hasStrictBounds ;
		lv2:minimum -60.000000 ;
		lv2:maximum 60.000000 ;
		lv2:default 20.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 9 ;
		lv2:symbol "time" ;
		lv2:name "Time" ;
		units:unit units:ms ;
		lv2:portProperty pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 1000.000000 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 10 ;
		lv2:symbol "dry" ;
		lv2:name "Dry amount" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 10.000000 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 11 ;
		lv2:symbol "wet" ;
		lv2:name "Wet amount" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 10.000000 ;
		lv2:default 1.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 12 ;
		lv2:symbol "drywet" ;
		lv2:name "Dry/Wet balance" ;
		units:unit units:pc ;
		lv2:portProperty pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 100.000000 ;
		lv2:default 100.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 13 ;
		lv2:symbol "phase" ;
		lv2:name "Phase Invert" ;
		lv2:portProperty lv2:toggled ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
		lv2:default 0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 14 ;
		lv2:symbol "g_out" ;
		lv2:name "Output gain" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 1000.000000 ;
		lv2:default 1.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 15 ;
		lv2:symbol "d_t" ;
		lv2:name "Delay time" ;
		units:unit units:ms ;
		lv2:portProperty pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 1000.000000 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 16 ;
		lv2:symbol "d_s" ;
		lv2:name "Delay samples" ;
		units:unit [
			a units:Unit ;
			rdfs:label "samples" ;
			units:symbol "samp" ;
			units:render "%.0f samp" ;
		] ;
		lv2:portProperty lv2:integer, pp:hasStrictBounds ;
		lv2:minimum 0 ;
		lv2:maximum 384000 ;
		lv2:default 0 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 17 ;
		lv2:symbol "d_d" ;
		lv2:name "Delay distance" ;
		units:unit units:cm ;
		lv2:portProperty pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 50000.000000 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:InputPort, atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Sequence, time:Position ;
		lv2:portProperty lv2:connectionOptional ;
		lv2:designation lv2:control ;
		lv2:index 18 ;
		lv2:symbol "in_ui" ;
		lv2:name "UI Input" ;
		rdfs:comment "UI -> DSP communication" ;
		rsz:minimumSize 4096 ;
	] , [
		a lv2:OutputPort, atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Sequence, time:Position ;
		lv2:portProperty lv2:connectionOptional ;
		lv2:designation lv2:control ;
		lv2:index 19 ;
		lv2:symbol "out_ui" ;
		lv2:name "UI Output" ;
		rdfs:comment "DSP -> UI communication" ;
		rsz:minimumSize 4096 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 20 ;
		lv2:symbol "out_latency" ;
		lv2:name "Latency OUT" ;
		lv2:designation lv2:latency ;
		rdfs:comment "DSP -> Host latency report" ;
		lv2:portProperty pp:hasStrictBounds, lv2:integer, lv2:reportsLatency ;
		lv2:minimum 0 ;
		lv2:maximum 384000 ;
		lv2:default 0 ;
	]
	.


