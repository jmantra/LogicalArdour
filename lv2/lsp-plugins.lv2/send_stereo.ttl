plug_pg:stereo_in
	a pg:StereoGroup, pg:InputGroup ;
	lv2:symbol "stereo_in" ;
	rdfs:label "Stereo Input" ;
	pg:element [
		lv2:index 0 ;
		lv2:designation pg:left
	] , [
		lv2:index 1 ;
		lv2:designation pg:right
	]
	.

plug_pg:stereo_out
	a pg:StereoGroup, pg:OutputGroup ;
	lv2:symbol "stereo_out" ;
	rdfs:label "Stereo Output" ;
	pg:element [
		lv2:index 0 ;
		lv2:designation pg:left
	] , [
		lv2:index 1 ;
		lv2:designation pg:right
	]
	.

plug_p:send
	a lv2:Parameter ;
	rdfs:label "Audio send connection point name" ;
	rdfs:range atom:String
	.

plug:send_stereo
	a lv2:UtilityPlugin, doap:Project ;
	doap:name "LSP Send Stereo" ;
	lv2:minorVersion 0 ;
	lv2:microVersion 2 ;
	doap:developer plug_dev:v_sadovnikov ;
	doap:maintainer plug_dev:lsp ;
	doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
	lv2:binary <lsp-plugins-lv2.so> ;
	ui:ui plug_ui:send_stereo ;
	lv2:requiredFeature urid:map ;
	lv2:optionalFeature lv2:hardRTCapable, hcid:queue_draw, work:schedule, opts:options, state:mapPath ;
	lv2:extensionData state:interface, work:interface, hcid:interface ;
	opts:supportedOption ui:updateRate ;
	pg:mainInput plug_pg:stereo_in ;
	pg:mainOutput plug_pg:stereo_out ;

	patch:writable plug_p:send ;

	lv2:port [
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 0 ;
		lv2:symbol "in_l" ;
		lv2:name "Input L" ;
		pg:group plug_pg:stereo_in ;
		lv2:designation pg:left ;
	] , [
		a lv2:InputPort, lv2:AudioPort ;
		lv2:index 1 ;
		lv2:symbol "in_r" ;
		lv2:name "Input R" ;
		pg:group plug_pg:stereo_in ;
		lv2:designation pg:right ;
	] , [
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 2 ;
		lv2:symbol "out_l" ;
		lv2:name "Output L" ;
		pg:group plug_pg:stereo_out ;
		lv2:designation pg:left ;
	] , [
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index 3 ;
		lv2:symbol "out_r" ;
		lv2:name "Output R" ;
		pg:group plug_pg:stereo_out ;
		lv2:designation pg:right ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "enabled" ;
		lv2:name "Enabled" ;
		lv2:designation lv2:enabled ;
		lv2:portProperty lv2:toggled ;
		lv2:minimum 0 ;
		lv2:maximum 1 ;
		lv2:default 1 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "g_in" ;
		lv2:name "Input gain" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 1000.000000 ;
		lv2:default 1.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "g_out" ;
		lv2:name "Output gain" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 1000.000000 ;
		lv2:default 1.000000 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "g_send" ;
		lv2:name "Send gain" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 1000.000000 ;
		lv2:default 1.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "ilm_l" ;
		lv2:name "Input level meter Left" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 15.848930 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 9 ;
		lv2:symbol "slm_l" ;
		lv2:name "Send level meter Left" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 15.848930 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 10 ;
		lv2:symbol "olm_l" ;
		lv2:name "Output level meter Left" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 15.848930 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 11 ;
		lv2:symbol "ilm_r" ;
		lv2:name "Input level meter Right" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 15.848930 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 12 ;
		lv2:symbol "slm_r" ;
		lv2:name "Send level meter Right" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 15.848930 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 13 ;
		lv2:symbol "olm_r" ;
		lv2:name "Output level meter Right" ;
		units:unit [
			a units:Unit ;
			rdfs:label "gain" ;
			units:symbol "G" ;
			units:render "%.8f G" ;
		] ;
		lv2:portProperty pp:logarithmic, pp:hasStrictBounds ;
		lv2:minimum 0.000000 ;
		lv2:maximum 15.848930 ;
		lv2:default 0.000000 ;
	] , [
		a lv2:InputPort, atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Sequence, patch:Message, time:Position ;
		lv2:portProperty lv2:connectionOptional ;
		lv2:designation lv2:control ;
		lv2:index 14 ;
		lv2:symbol "in_ui" ;
		lv2:name "UI Input" ;
		rdfs:comment "UI -> DSP communication" ;
		rsz:minimumSize 3072 ;
	] , [
		a lv2:OutputPort, atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Sequence, patch:Message, time:Position ;
		lv2:portProperty lv2:connectionOptional ;
		lv2:designation lv2:control ;
		lv2:index 15 ;
		lv2:symbol "out_ui" ;
		lv2:name "UI Output" ;
		rdfs:comment "DSP -> UI communication" ;
		rsz:minimumSize 3072 ;
	] , [
		a lv2:OutputPort, lv2:ControlPort ;
		lv2:index 16 ;
		lv2:symbol "out_latency" ;
		lv2:name "Latency OUT" ;
		lv2:designation lv2:latency ;
		rdfs:comment "DSP -> Host latency report" ;
		lv2:portProperty pp:hasStrictBounds, lv2:integer, lv2:reportsLatency ;
		lv2:minimum 0 ;
		lv2:maximum 384000 ;
		lv2:default 0 ;
	]
	.


